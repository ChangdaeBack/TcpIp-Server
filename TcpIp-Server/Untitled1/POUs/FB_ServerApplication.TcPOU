    <Method Name="GetLogData" Id="{2fca1445-3bd3-4cd4-be7c-f9e39800af7a}">
      <Declaration><![CDATA[METHOD GetLogData
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[fbLog.Remove( );
IF fbLog.bOk THEN
	
	(* TODO: Implement log message handler *)
	sLogMessage := fbLog.sGet;
END_IF]]></ST>
