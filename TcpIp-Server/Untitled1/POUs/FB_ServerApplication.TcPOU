	_Updated : BOOL;
	
	sFromClient	: T_MaxString := '';
	sToClient	: T_MaxString := '';
	sLogMessage : T_MaxString := '';
	nCounter	: UDINT;
	nTempCounter : UDINT;
	
	ReceiveandSendData : I_Application;
END_VAR]]></Declaration>
    <Implementation>
